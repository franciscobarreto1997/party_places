<br>
<center><h2>Your Bookings</h2></center>
<br>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Price</th>
      <th scope="col">Address</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <% current_user.bookings.each do |booking| %>
      <td> <%= booking.venue.name %> </td>
      <td> <%= booking.venue.price %> </td>
      <td> <%= booking.venue.address %> </td>
      <td> <%= link_to "Add a Review" , booking %></td>
      <td> <%= link_to "Show Venue" , venue_path(booking.venue) %></td>


    </tr>
    <% end %>
    <tr>
  </tbody>
</table>
<br>
<center><%= link_to 'Home', root_path %></center>
<br>
